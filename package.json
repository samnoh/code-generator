{
    "name": "@samnoh/react-gen",
    "version": "1.0.8",
    "description": "React component generator",
    "main": "index.js",
    "scripts": {
        "start": "node index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samnoh/code-generator.git"
    },
    "keywords": [
        "react",
        "typescript",
        "code generator"
    ],
    "author": "Sam Noh <samnoh.dev@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/samnoh/code-generator/issues"
    },
    "homepage": "https://github.com/samnoh/code-generator#readme",
    "config": {
        "reactgen": {
            "defaultName": "Component",
            "modules": {
                "styled": "styled-components",
                "{ Link }": "react-router-dom"
            },
            "typescript": false,
            "classBased": false,
            "template": {
                "LAZY": "React.lazy(() => import(''));",
                "USEEFFECT": "useEffect(() => {}, []);"
            }
        }
    },
    "dependencies": {
        "commander": "4.0.1"
    },
    "bin": {
        "reactgen": "index.js"
    }
}
